<job>
    <Name>LeNLeMixed_Main_Standard</Name>
    <Tmp>/tmpfs</Tmp>
    <Log_Dir>/log</Log_Dir>
    <Log>0</Log>
    <Log_Lgs>LeNLeMixed_Main_Standard_TIME_</Log_Lgs>
    <Log_Excel>LeNLeMixed_Main_Standard_TIME_</Log_Excel>
    <NLe>
        <Timeout>10</Timeout>
        <TestRound>100</TestRound>
        <CBC_Limit>1000</CBC_Limit>
        <UseAdvancedCBC>True</UseAdvancedCBC>
        <MLearnTarget>L2</MLearnTarget>
        <MLearnClusterNum>10</MLearnClusterNum>
    </NLe>
    <Le>
        <Timeout>25</Timeout>
        <TestRound>100</TestRound>
        <UseAdvancedCBC>True</UseAdvancedCBC>
        <LexicalStrategy>TraceBest</LexicalStrategy>
        <LexicalRankMax>2</LexicalRankMax>
        <MLearnTarget>LSM_L2</MLearnTarget>
    </Le>
    <Le>
        <Timeout>40</Timeout>
        <TestRound>100</TestRound>
        <UseAdvancedCBC>True</UseAdvancedCBC>
        <LexicalStrategy>TraceBest</LexicalStrategy>
        <LexicalRankMax>3</LexicalRankMax>
        <MLearnTarget>LSM_L2</MLearnTarget>
    </Le>
</job>