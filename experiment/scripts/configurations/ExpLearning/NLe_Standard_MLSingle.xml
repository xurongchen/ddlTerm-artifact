<job>
    <Name>NLe_Standard_MLSingle</Name>
    <Tmp>/tmpfs</Tmp>
    <Log_Dir>/log</Log_Dir>
    <Log>0</Log>
    <Log_Lgs>NLe_Standard_MLSingle_TIME_</Log_Lgs>
    <Log_Excel>NLe_Standard_MLSingle_TIME_</Log_Excel>
    <NLe>
        <Timeout>60</Timeout>
        <TestRound>100</TestRound>
        <CBC_Limit>1000</CBC_Limit>
        <UseAdvancedCBC>True</UseAdvancedCBC>
        <MLearnTarget>L2</MLearnTarget>
        <MLearnClusterNum>10</MLearnClusterNum>
        <MLearnStrategy>single-best</MLearnStrategy>
    </NLe>
</job>